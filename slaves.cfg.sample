# Each section is a slave name
# A closed list of options turns into slaves parameters
# the others go in build properties

[purity.anybox.eu]
# required
password = purity
pg_version = 9.1

# optional parameters
max_builds = 2
notify_on_missing = 'joe@example.com'

# optional build properties
pg_port = 5433
db_prefix = oebb # builder name will be appended to it
